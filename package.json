{
  "name": "crashlogger",
  "version": "1.0.0",
  "description": "Converts production file paths into source code paths based on sourcemaps",
  "license": "MIT",
  "author": "RokuCommunity",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test:nocover": "mocha"
  },
  "mocha": {
    "spec": "src/**/*.spec.ts",
    "require": [
      "source-map-support/register",
      "ts-node/register"
    ],
    "fullTrace": true,
    "watchExtensions": [
      "ts"
    ]
  },
  "files": [
    "dist/**/!(*.spec.*)*",
    "bslib.brs",
    "bsconfig.schema.json"
  ],
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.18",
    "@types/sinon": "^10.0.11",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "chai": "^4.3.6",
    "eslint": "^8.9.0",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "mocha": "^9.2.0",
    "nyc": "^15.1.0",
    "sinon": "^13.0.1",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "brighterscript": "^0.45.0",
    "extract-zip": "^2.0.1",
    "fs-extra": "^10.0.0",
    "glob-all": "^3.2.1",
    "yargs": "^17.3.1"
  }
}
